<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  
    
    <welcome-file-list>
        <welcome-file>Welcome.html</welcome-file>
    </welcome-file-list>
    
    <!-- Endpoints servlets -->
    
 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value>com.tudor.ctm.endpoint.CTMUserEndpoint,com.tudor.ctm.endpoint.CloudTaskEndpoint,com.tudor.ctm.endpoint.DeviceInfoEndpoint</param-value>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
 
  <listener>
    <listener-class>
      com.tudor.ctm.dm.ApiKeyInitializer
    </listener-class>
  </listener>

  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>
      com.tudor.ctm.dm.RegisterServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UnregisterServlet</servlet-name>
    <servlet-class>
      com.tudor.ctm.dm.UnregisterServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>UnregisterServlet</servlet-name>
    <url-pattern>/unregister</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SendAllMessagesServlet</servlet-name>
    <servlet-class>
      com.tudor.ctm.dm.SendAllMessagesServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>SendAllMessagesServlet</servlet-name>
    <url-pattern>/sendAll</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SendMessageServlet</servlet-name>
    <servlet-class>
      com.tudor.ctm.dm.SendMessageServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>SendMessageServlet</servlet-name>
    <url-pattern>/send</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>
      com.tudor.ctm.dm.HomeServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  
  
<!--   RPC servlets --> 

  <servlet>
    <servlet-name>manageTaskServlet</servlet-name>
    <servlet-class>com.tudor.ctm.ui.server.ManageTaskServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>manageTaskServlet</servlet-name>
    <url-pattern>/com.tudor.ctm/manageTask</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>getUserTasks</servlet-name>
    <servlet-class>com.tudor.ctm.ui.server.GetUserTasksImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>getUserTasks</servlet-name>
    <url-pattern>/com.tudor.ctm/getUserTasks</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>getUserData</servlet-name>
    <servlet-class>com.tudor.ctm.ui.server.GetUserDataImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>getUserData</servlet-name>
    <url-pattern>/com.tudor.ctm/getUserData</url-pattern>
  </servlet-mapping>
 
</web-app>