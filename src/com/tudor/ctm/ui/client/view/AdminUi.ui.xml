<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
        <g:HorizontalPanel ui:field="hPanel">
        	<g:VerticalPanel>
        			<g:HorizontalPanel>
        				<g:Button ui:field="btnAddProject">Add Project</g:Button>
        			</g:HorizontalPanel>
        			<g:ScrollPanel width="250px" height="500px">
        				<g:VerticalPanel ui:field="vPanelProjects" width="auto" height="auto"/>
        			</g:ScrollPanel>
        		</g:VerticalPanel>
	        <g:VerticalPanel ui:field="vPanelR">
	        	<g:Grid>
	        		<g:row>
	        			<g:customCell>
	        				<g:Label text="Coordinator"/>
	        			</g:customCell>
	        			<g:customCell>
	        				<g:ListBox ui:field="coordinator" width="100%"></g:ListBox>
	        			</g:customCell>
	        		</g:row>
	        		<g:row>
	        			<g:customCell>
	        				<g:Label text="Members"/>
	        			</g:customCell>
	        			<g:customCell>
	        				<g:ListBox ui:field="members" multipleSelect="true" width="100%"></g:ListBox>
	        			</g:customCell>
	        		</g:row>
	        		<g:row>
	        			<g:customCell>
	        				<g:Button ui:field="btnSave" text="Save" width="59px"/>
	        			</g:customCell>
	        		</g:row>
	        	</g:Grid>
	        </g:VerticalPanel>    
        </g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 