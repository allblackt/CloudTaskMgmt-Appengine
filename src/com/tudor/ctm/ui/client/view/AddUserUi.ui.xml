<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel ui:field="cbAdmin">
		<g:Grid>
			<g:row>
				<g:cell>Pick User</g:cell>
				<g:customCell>
					<g:ListBox ui:field="lstUsers" width="169px"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Name"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="txtName"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Email"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="txtEmail"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Admin"/>
				</g:customCell>
				<g:customCell>
					<g:SimpleCheckBox ui:field="checkAdmin"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Button text="Save" ui:field="btnSave"/>
				</g:customCell>
				<g:customCell>
					<g:Button ui:field="btnCancel" text="Cancel"/>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 